layer{
	name:	"rectify"
	type:	RECTIFY
	top:	"adaboost"
	rectify_param{
		filename:	"F:/svaf/data/calib/stereo_rectify_180309.dat"
	}
}
layer{
	name:	"adaboost"
	type:	ADABOOST
	top:	"eadp"
	adaboost_param{
		detector:	"F:/svaf/data/boost/detector_workpiece_180403.dat"
		sync_frame:	true
		#sync_video:	true
		sync_epipolar:	true
		thresh:		40
		nms:		0.3
	}
	show: true
}
layer{
	name:	"eadp"
	type:	EADP_MATCH
	top:	"3dpoint"
	eadp_param{
		max_disp:	24
		factor:		2560
		#r1:			30
		#r2:			500
	}
	show:	true
	#logt:	true
}
layer{
	name:	"3dpoint"
	type:	TRIANG
	top:	"camera2world"
	triang_param{
		visible:	false
		savepc:		true
		toolbox_dir:	"F:/svaf/calib/"
		calibmat_dir:	"F:/svaf/data/calib/mat_161103/"
