layer{
	name:	"featpoint2"
	type:	CV_POINT
	top:	"featpoint2"
	cvpoint_param{
		type:	FAST # FAST = 1;FASTX = 2;MSER = 3;ORB = 4;BRISK = 5;FREAK = 6;STAR = 7;SIFT = 8;SURF = 9;GFTT = 10;HARRIS = 11;DENSE = 12;SBLOB = 13;AKAZE = 14;
		fast_param{
			thresh:	5
			isnms:	true
			ftype:	T_5_8 # T_9_16 = 1;T_7_12 = 2;T_5_8 = 3;
		}
		mser_param{
			delta:		5
			min_area:	60
			max_area:	14400
			max_varia:	0.25
			min_divers:	0.2
			max_evolution:	200
			area_thresh:	1.01
			min_margin:		0.003
			edgeblur_size:	5
		}
		orb_param{
			nfeatures:	500
			scalefactor: 1.2
			nlevels:	3
			edgethresh:	3
			firstlevel:	0
			wta_k:		2
			scoretype:	HARRIS_SCORE   #HARRIS_SCORE = 1;FAST_SCORE = 2;
			patchsize:	31
		}
		brisk_param{
			thresh:		30
			octaves:	3
			ptscale:	1.0
			#dmax:		5.85
			#dmin:		8.2
		}
		star_param{
			maxsize:	16
			response_thresh:	30
			projected_thresh:	10
			binarized_thresh:	8
			nms_size:	5
		}
		sift_param{
			nfeatures:	0
			octaves:	2
			contrast_thresh:	0.04
			edge_thresh:	10
			sigma:		1.6
		}
		surf_param{
			hassian_thresh:	0.0004
			octaves:	4
			intevals:	2
			extended:	false
			upright:	false
		}
		gftt_param{
			maxcornners:	1000
			quality:	0.01
			mindist:	1
			blocksize:	3
			useharris:	false
			k:			0.04
		}
		harris_param{
			maxcornners:	1000
			quality:	0.01
			mindist:	1
			blocksize:	3
			k:			0.04
		}
		dense_param{
			initfeatscale:	1
			featscalelevel:	1
			featscalemul:	0.1
			initxystep:		6
			initbound:		0
			varyxyscale:	true
			varybdscale:	false
		}
	}
	display: true
}




