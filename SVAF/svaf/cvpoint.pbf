name:	"OpenCV Feature Point Detect"
version:	"v_1.04"
pause:	0
layer{
	name:	"data"
	type:	IMAGE_PAIR_FOLDER
	bottom:	"data"
	top:	"rectify"
	data_param{
		color: true
	}
	pairfolder_param{
		pair{
			left:	"F:/data/BaseData/left/"
			right:	"F:/data/BaseData/right/"
		}
	}
}
layer{
	name:	"rectify"
	type:	RECTIFY
	top:	"adaboost"
	rectify_param{
		filename:	"F:/svaf/data/calib/stereo_rectify_161103.dat"
	}
}
layer{
	name:	"adaboost"
	type:	ADABOOST
	top:	"featpoint"
	adaboost_param{
		detector:	"F:/svaf/data/boost/detector_workpiece_161103.dat"
		sync_frame:	true
		#sync_video:	true
		epipolar:	true
		thresh:		40
		nms:		0.3
	}
}
layer{
	name:	"featpoint"
	type:	CV_POINT
	top:	"featpoint"
	cvpoint_param{
		type:	SURF # FAST = 1;FASTX = 2;MSER = 3;ORB = 4;BRISK = 5;FREAK = 6;STAR = 7;SIFT = 8;SURF = 9;GFTT = 10;HARRIS = 11;DENSE = 12;SBLOB = 13;
	}
}

